{
  "name": "desgruda",
  "displayName": "Desgruda - Separate Sibling Elements in JSX/TSX code",
  "description": "Improve code readability by automatically separating sibling JSX/TSX elements with a blank line.",
  "icon": "images/icon.png",
  "version": "1.0.0-alpha.1",
  "publisher": "isaacmuniz.pro",
  "repository": "https://github.com/codigoisaac/Desgruda",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "desgruda.desgruda",
        "title": "Desgruda"
      }
    ],
    "keybindings": [
      {
        "command": "desgruda.desgruda",
        "key": "ctrl+d g",
        "mac": "cmd+d g",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "magic-string": "^0.30.21"
  }
}
